    "@types/common-tags": "1.8.4",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/parser": "8.48.0",
    "babel-plugin-react-compiler": "1.0.0",
