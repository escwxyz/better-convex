    "@convex-dev/resend": "0.1.13",
    "@hookform/resolvers": "5.2.2",
    "@react-email/components": "1.0.1",
    "@react-email/render": "2.0.0",
    "@t3-oss/env-nextjs": "0.13.8",
    "@tanstack/react-query": "5.90.6",
    "better-auth": "1.3.27",
    "better-auth-convex": "0.4.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "common-tags": "1.8.2",
    "convex": "1.28.2",
    "convex-ents": "0.16.0",
    "convex-helpers": "0.1.105",
    "date-fns": "4.1.0",
    "dotenv": "17.2.3",
    "embla-carousel-react": "8.6.0",
    "input-otp": "1.4.2",
    "jotai-x": "2.3.3",
    "lucide-react": "0.554.0",
    "next": "16.0.4",
    "next-themes": "0.4.6",
    "nuqs": "2.8.1",
    "radix-ui": "latest",
    "react": "19.2.0",
    "react-day-picker": "9.11.2",
