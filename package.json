    "superjson": "2.2.5",
    "tailwind-merge": "3.4.0",
    "ts-essentials": "10.1.1",
    "tsx": "4.20.6",
    "type-fest": "5.2.0",
    "use-debounce": "10.0.6",
    "vaul": "1.1.2",
    "zod": "4.1.13"
